<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.carshare">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CarRental">
        <activity
            android:name="com.example.carshare.loginmethods.LoginMethodsActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.CarRental.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.example.carshare.login.LoginActivity"
            android:parentActivityName=".loginmethods.LoginMethodsActivity"
            android:label="@string/login">
        </activity>

        <activity
            android:name="com.example.carshare.bottomnavigation.BottomNavigationActivity"
            android:label="@string/bottom_navigation">
        </activity>
    </application>

</manifest>